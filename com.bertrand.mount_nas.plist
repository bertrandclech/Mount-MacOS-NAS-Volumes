<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bertrand.mount_nas</string>
    <key>Program</key>
	<string>/usr/bin/osascript</string>
    <key>ProgramArguments</key>
    <array>
		<string>osascript</string>
        <string>/Users/Shared/scripts/mountNAS.scpt</string>
    </array>
    <key>OnDemand</key>
	<true/>
    <key>RunAtLoad</key>
	<true/>
    <key>StartInterval</key>
    <integer>30</integer>
    <key>Debug</key>
    <true/>
</dict>
</plist>
